  "astro.content-intellisense": true,
  "biome.enabled": true,
  "commitlint.config.file": "commitlint.config.ts",
  "cSpell.autoFormatConfigFile": true,
  "cSpell.enabled": true,
  "cSpell.files": [
    "**/*",
    "**/.*",
    "**/.*/**/*",
    "**/.*/**/.*",
    "**/.*/**/.*/**/*",
    "**/.*/**/.*/**/.*",
    "**/.git/COMMIT_EDITMSG"
  ],
  "cSpell.language": "en,en-GB,en-US,uk",
  "debug.javascript.defaultRuntimeExecutable": {
    "pwa-node": "c:\\users\\vs\\appdata\\local\\mise\\shims\\node"
  },
  "editor.codeActionsOnSave": [
    "source.formatDocument",
    "source.organizeImports.biome",
    "source.fixAll.markdownlint",
    "source.fixAll.stylelint",
    "source.fixAll.biome",
    "source.fixAll.oxc",
    "source.fixAll.eslint"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.rulers": [
    80,
    100,
    120
  ],
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "ejs": "html",
    "javascript": "javascriptreact",
    "markdown": "html",
    "typescript": "typescriptreact",
    "vue-html": "html"
  },
  "eslint.lintTask.options": "--max-warnings 0 --no-warn-ignored --report-unused-disable-directives-severity warn",
  "eslint.onIgnoredFiles": "off",
  "eslint.probe": [
    "astro",
    "css",
    "gql",
    "graphql",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "less",
    "markdown",
    "pcss",
    "postcss",
    "scss",
    "svelte",
    "toml",
    "typescript",
    "typescriptreact",
    "vue",
    "xml",
    "yaml"
  ],
  "eslint.rules.customizations": [
    {
      "rule": "*",
      "severity": "warn"
    }
  ],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "exportall.config.folderListener": [
    "/packages/eslint-config/src/configs"
  ],
  "markdown.validate.duplicateLinkDefinitions.enabled": "warning",
  "markdown.validate.enabled": true,
  "markdown.validate.fileLinks.enabled": "ignore",
  "markdown.validate.fileLinks.markdownFragmentLinks": "ignore",
  "markdown.validate.fragmentLinks.enabled": "ignore",
  "markdown.validate.referenceLinks.enabled": "ignore",
  "markdown.validate.unusedLinkDefinitions.enabled": "hint",
  "oxc.enable": true,
  "oxc.flags": {
    "disable_nested_config": false,
    "fix_kind": "all"
  },
  "oxc.lint.run": "onType",
  "oxc.unusedDisableDirectives": "allow",
  "prettier.documentSelectors": [
