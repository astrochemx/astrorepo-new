  {
    "key": "ctrl+alt+i",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
      "snippet": "\n```{r <name>, cache = FALSE, echo = FALSE, eval = TRUE, include = FALSE, message = FALSE, tidy = TRUE, results = \"asis\", size = \"small\", warning = FALSE, ft.arraystretch = 1.5, ft.latex.float = \"none\", ft.tabcolsep = 0}\n$0\n```\n"
    }
  },
  {
    "key": "right",
    "command": "-gifPlayer.nextFrame",
    "when": "gifPlayerActive"
  },
  {
    "key": "left",
    "command": "-gifPlayer.previousFrame",
    "when": "gifPlayerActive"
  },
  {
    "key": "space",
    "command": "-gifPlayer.togglePlay",
    "when": "gifPlayerActive"
  },
  {
    "key": "ctrl+d ctrl+b",
    "command": "-dbcode.connections.view.focus"
  },
  {
    "key": "alt+[",
    "command": "-print-it.PrintIt"
  },
  {
    "key": "ctrl+z",
    "command": "-zotex.addCitation",
    "when": "editorTextFocus && editorLangId == 'latex' || editorTextFocus && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+alt+z",
    "command": "-zotex.addCiteBib",
    "when": "editorTextFocus && editorLangId == 'latex' || editorTextFocus && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+s",
    "command": "-zotex.addZoteroSelectedCitation",
    "when": "editorTextFocus && editorLangId == 'latex' || editorTextFocus && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+alt+s",
    "command": "-zotex.addZoteroSelectedCitationAndBibliography",
    "when": "editorTextFocus && editorLangId == 'latex' || editorTextFocus && editorLangId == 'markdown'"
  }
]
