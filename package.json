{
  "name": "@astrochemx/astrorepo",
  "version": "0.0.0",
  "private": true,
  "description": "@astrochemx's astrorepo âœ¨",
  "keywords": [
    "astro",
    "biome",
    "commitlint",
    "config",
    "configs",
    "cspell",
    "docs",
    "documentation",
    "eslint",
    "knip",
    "oxlint",
    "page",
    "personal",
    "portfolio",
    "prettier",
    "projects",
    "starlight",
    "stylelint",
    "tailwindcss",
    "ts",
    "tsconfig",
    "typescript",
    "website"
  ],
  "homepage": "https://github.com/astrochemx/astrorepo#readme",
  "bugs": {
    "url": "https://github.com/astrochemx/astrorepo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astrochemx/astrorepo.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/astrochemx"
  },
  "license": "(MIT OR CC-BY-SA-4.0)",
  "author": {
    "name": "Vladyslav Shevchenko",
    "url": "https://github.com/astrochemx"
  },
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "audit": "pnpm audit --audit-level low",
    "audit:fix": "pnpm audit --fix --audit-level low",
    "build": "turbo run build",
    "build:apps": "turbo run build --filter=\"./apps/*\"",
    "build:pkgs": "turbo run build --filter=\"./packages/*\"",
    "changeset": "changeset",
    "changeset:init": "changeset init",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "changeset:version": "changeset version",
    "check": "pnpm run \"/^check:(biome|eslint|knip|md|oxlint|prettier|spell|stylelint|types)\\$/\"",
    "check:all": "pnpm run \"/^check:(biome|dedupe|eslint|knip|md|oxlint|pkg|prettier|spell|stylelint|types)\\$/\"",
    "check:attw": "attw --pack . --ignore-rules cjs-resolves-to-esm",
    "check:biome": "biome check --max-diagnostics 200",
    "check:biome:format": "biome format --max-diagnostics 200",
    "check:biome:lint": "biome lint --max-diagnostics 200",
    "check:dedupe": "pnpm dedupe --check",
    "check:eslint": "eslint --max-warnings 0 --no-warn-ignored --report-unused-disable-directives-severity warn",
    "check:knip": "knip",
    "check:lockfile": "node ./scripts/check-lockfile.mjs",
    "check:md": "markdownlint \"**/*.md\" \".github/**/*.md\"",
    "check:md:cli2": "markdownlint-cli2 \"**/*.md\" \".github/**/*.md\"",
    "check:npm": "pnpm pack --recursive && attw . --ignore-rules cjs-resolves-to-esm",
    "check:oxlint": "oxlint --report-unused-disable-directives-severity=off",
    "check:pkg": "sort-package-json --check \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "check:prettier": "prettier --check --ignore-unknown .",
    "check:spell": "cspell lint .",
    "check:spell:words": "cspell lint --words-only --unique --no-progress .",
    "check:stylelint": "stylelint --allow-empty-input --compute-edit-info --max-warnings 0 --quiet-deprecation-warnings --report-descriptionless-disables --report-invalid-scope-disables --report-needless-disables --report-unscoped-disables \"**/*.{astro,css,html,js,jsx,md,pcss,postcss,scss,styl,stylus,svelte,ts,tsx,vue}\"",
    "check:types": "tsc --noEmit",
    "clean:all": "pnpm run clean:temp ; pnpm run clean:deps",
    "clean:deps": "del-cli \"**/node_modules\" \"**/*-lock.{json,yaml}\" \"**/*.lock\"",
    "clean:temp": "del-cli \"**/*-dist\" \"**/.*-inspect\" \"**/.*-inspector\" \"**/.astro*\" \"**/.content-collections\" \"**/.contentlayer\" \"**/.eslint-*\" \"**/.husky/_\" \"**/.next*\" \"**/.nuxt*\" \"**/.source*\" \"**/.svelte*\" \"**/.tsbuildinfo\" \"**/.turbo*\" \"**/.vercel\" \"**/.vite*\" \"**/.vscode-*\" \"**/build\" \"**/coverage\" \"**/dist\" \"**/dist-*\" \"**/next-env.d.ts\" \"**/out\" \"**/typegen.d.ts\"",
    "contributors:check": "all-contributors check",
    "contributors:gen": "all-contributors generate",
    "corepack:enable-pnpm": "corepack enable pnpm",
    "corepack:init": "npm install --global corepack@latest",
    "corepack:install-pnpm": "corepack install --global pnpm@latest",
    "corepack:use-pnpm": "corepack use pnpm@latest",
    "cz": "czg",
    "dev": "turbo run dev",
    "dev:apps": "turbo run dev --filter=\"./apps/*\"",
    "dev:host": "turbo run dev:host",
    "dev:host:apps": "turbo run dev:host --filter=\"./apps/*\"",
    "dev:host:pkgs": "turbo run dev:host --filter=\"./packages/*\"",
    "dev:pkgs": "turbo run dev --filter=\"./packages/*\"",
    "dlx:check:attw": "pnpm dlx @arethetypeswrong/cli --pack . --ignore-rules cjs-resolves-to-esm",
    "dlx:check:npm": "pnpm dlx @arethetypeswrong/cli --pack .",
    "dlx:check:pkg": "pnpm dlx sort-package-json@latest --check \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "dlx:check:spell": "pnpm dlx cspell-cli@latest lint .",
    "dlx:check:spell:words": "pnpm dlx cspell-cli@latest lint . --words-only --unique --no-progress",
    "dlx:clean:all": "pnpm run dlx:clean:temp ; pnpm run dlx:clean:deps",
    "dlx:clean:deps": "pnpm dlx del-cli@latest \"**/node_modules\" \"**/*-lock.{json,yaml}\" \"**/*.lock\"",
    "dlx:clean:temp": "pnpm dlx del-cli@latest \"**/*-dist\" \"**/.*-inspect\" \"**/.*-inspector\" \"**/.astro*\" \"**/.content-collections\" \"**/.contentlayer\" \"**/.eslint-*\" \"**/.husky/_\" \"**/.next*\" \"**/.nuxt*\" \"**/.source*\" \"**/.svelte*\" \"**/.tsbuildinfo\" \"**/.turbo*\" \"**/.vercel\" \"**/.vite*\" \"**/.vscode-*\" \"**/build\" \"**/coverage\" \"**/dist\" \"**/dist-*\" \"**/next-env.d.ts\" \"**/out\" \"**/typegen.d.ts\"",
    "dlx:fix:pkg": "pnpm dlx sort-package-json@latest \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "dlx:init:oxlint": "pnpm dlx oxlint@latest --import-plugin --jest-plugin --jsdoc-plugin --jsx-a11y-plugin --nextjs-plugin --node-plugin --promise-plugin --react-perf-plugin --react-plugin --regex-plugin --vitest-plugin --vue-plugin --tsconfig=./tsconfig.json --init",
    "dlx:upg": "pnpm run dlx:upg:taze ; pnpm run dlx:upg:ncu",
    "dlx:upg:full": "pnpm run dlx:upg && pnpm install && pnpm run upg:pnpm && pnpm run fix:dedupe",
    "dlx:upg:ncu": "pnpm dlx npm-check-updates@latest --workspaces --root --upgrade",
    "dlx:upg:ncu:ls": "pnpm dlx npm-check-updates@latest --workspaces --root",
    "dlx:upg:taze": "pnpm dlx taze@latest --recursive --write",
    "dlx:upg:taze:ls": "pnpm dlx taze@latest --recursive",
    "fix": "pnpm run \"/^(fix:(biome|eslint|md|oxlint|pkg|prettier|stylelint)|check:types)\\$/\"",
    "fix:all": "pnpm run \"/^(fix:(biome|dedupe|eslint|md|oxlint|pkg|prettier|stylelint)|check:types)\\$/\"",
    "fix:biome": "biome check --write --max-diagnostics 200",
    "fix:biome:format": "biome format --write --max-diagnostics 200",
    "fix:biome:lint": "biome lint --write --max-diagnostics 200",
    "fix:biome:unsafe": "biome check --write --unsafe --max-diagnostics 200",
    "fix:dedupe": "pnpm dedupe",
    "fix:eslint": "eslint --fix --max-warnings 0 --no-warn-ignored --report-unused-disable-directives-severity warn",
    "fix:knip": "knip --fix",
    "fix:md": "markdownlint --fix \"**/*.md\" \".github/**/*.md\"",
    "fix:md:cli2": "markdownlint-cli2 --fix \"**/*.md\" \".github/**/*.md\"",
    "fix:mise": "mise fmt",
    "fix:oxlint": "oxlint --fix --report-unused-disable-directives-severity=off",
    "fix:pkg": "sort-package-json \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "fix:stylelint": "stylelint --fix --allow-empty-input --max-warnings 0 --quiet-deprecation-warnings --report-descriptionless-disables --report-invalid-scope-disables --report-needless-disables --report-unscoped-disables \"**/*.{astro,css,html,js,jsx,md,pcss,postcss,scss,styl,stylus,svelte,ts,tsx,vue}\"",
    "fix:unsafe": "pnpm run \"/^(fix:(biome:unsafe|eslint|md|oxlint|prettier|stylelint)|check:types)\\$/\"",
    "init:husky": "pnpm dlx husky@latest",
    "init:oxlint": "oxlint --import-plugin --jest-plugin --jsdoc-plugin --jsx-a11y-plugin --nextjs-plugin --node-plugin --promise-plugin --react-perf-plugin --react-plugin --regex-plugin --vitest-plugin --vue-plugin --tsconfig=./tsconfig.json --init",
    "init:sgh": "pnpm dlx simple-git-hooks@latest",
    "inspect:eslint": "eslint-config-inspector",
    "inspect:modules": "node-modules-inspector",
    "postinstall": "pnpm run build:pkgs",
    "install:corepack": "npm install --global corepack@latest",
    "install:husky": "husky || true",
    "install:husky:script": "node ./scripts/prepare-husky.mjs",
    "install:pnpm": "npm install --global pnpm@latest",
    "install:pnpm:exe": "npm install --global @pnpm/exe@latest",
    "install:puppeteer": "pnpm dlx puppeteer browsers install",
    "preinstall:disabled": "npx --yes only-allow@latest pnpm",
    "prepare:disabled": "node ./scripts/prepare-husky.mjs",
    "test:vi": "vitest",
    "test:vi:cov": "vitest --coverage",
    "test:vi:cov:ui": "vitest --coverage --ui",
    "test:vi:tsc": "vitest --typecheck",
    "test:vi:tsc:ui": "vitest --typecheck --ui",
    "test:vi:ui": "vitest --ui",
    "turbo:telemetry:disable": "turbo telemetry disable",
    "turbo:telemetry:enable": "turbo telemetry enable",
    "turbo:telemetry:status": "turbo telemetry status",
    "upd": "pnpm update --recursive",
    "upd:ls": "pnpm outdated --recursive",
    "upg": "pnpm run upg:taze ; pnpm run upg:ncu ; pnpm run upg:pnpm",
    "upg:full": "pnpm run upg && pnpm run fix:dedupe",
    "upg:ncu": "npm-check-updates --workspaces --root --upgrade",
    "upg:ncu:ls": "npm-check-updates --workspaces --root",
    "upg:pnpm": "pnpm update --recursive --latest",
    "upg:pnpm:ls": "pnpm run upd:ls",
    "upg:taze": "taze --recursive --write",
    "upg:taze:ls": "taze --recursive"
  },
