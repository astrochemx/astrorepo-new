{
  "name": "@astrochemx/astrorepo",
  "version": "0.0.0",
  "private": true,
  "description": "@astrochemx's astrorepo âœ¨",
  "keywords": [
    "astro",
    "biome",
    "commitlint",
    "config",
    "configs",
    "cspell",
    "docs",
    "documentation",
    "eslint",
    "knip",
    "oxlint",
    "page",
    "personal",
    "portfolio",
    "prettier",
    "projects",
    "starlight",
    "stylelint",
    "tailwindcss",
    "ts",
    "tsconfig",
    "typescript",
    "website"
  ],
  "homepage": "https://github.com/astrochemx/astrorepo#readme",
  "bugs": {
    "url": "https://github.com/astrochemx/astrorepo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astrochemx/astrorepo.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/astrochemx"
  },
  "license": "(MIT OR CC-BY-SA-4.0)",
  "author": {
    "name": "Vladyslav Shevchenko",
    "url": "https://github.com/astrochemx"
  },
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:apps": "turbo run build --filter=\"./apps/*\"",
    "build:pkgs": "turbo run build --filter=\"./packages/*\"",
    "changeset": "changeset",
    "changeset:init": "changeset init",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "changeset:version": "changeset version",
    "check": "pnpm run \"/^check:(biome|eslint|knip|md|oxlint|prettier|spell|stylelint|types)\\$/\"",
    "check:all": "pnpm run \"/^check:(biome|eslint|knip|md|oxlint|pkg|pkgs|prettier|spell|stylelint|types)\\$/\"",
    "check:attw": "attw --pack . --ignore-rules cjs-resolves-to-esm",
    "check:biome": "biome check --max-diagnostics 100",
    "check:biome:format": "biome format --max-diagnostics 100",
    "check:biome:lint": "biome lint --max-diagnostics 100",
    "check:eslint": "eslint --max-warnings 0 --no-warn-ignored --report-unused-disable-directives-severity warn",
    "check:knip": "knip",
    "check:lockfile": "node ./scripts/lockfile-check.mjs",
    "check:md": "markdownlint \"**/*.md\" \".github/**/*.md\"",
    "check:npm": "pnpm pack --recursive && attw . --ignore-rules cjs-resolves-to-esm",
    "check:oxlint": "oxlint --report-unused-disable-directives-severity=off",
    "check:pkg": "sort-package-json --check \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "check:pkgs": "pnpm dedupe --check",
    "check:prettier": "prettier --check --ignore-unknown .",
    "check:spell": "cspell lint .",
    "check:spell:words": "cspell lint --words-only --unique --no-progress .",
    "check:stylelint": "stylelint --allow-empty-input --compute-edit-info --max-warnings 0 --quiet-deprecation-warnings --report-descriptionless-disables --report-invalid-scope-disables --report-needless-disables --report-unscoped-disables \"**/*.{astro,css,html,js,jsx,md,pcss,postcss,scss,styl,stylus,svelte,ts,tsx,vue}\"",
    "check:types": "tsc --noEmit",
    "clean:all": "pnpm run clean:temp ; pnpm run clean:deps",
    "clean:deps": "del-cli \"**/node_modules\" \"**/*-lock.{json,yaml}\" \"**/*.lock\"",
    "clean:temp": "del-cli \"**/.*-inspect\" \"**/.*-inspector\" \"**/.astro*\" \"**/.content-collections\" \"**/.contentlayer\" \"**/.eslint-*\" \"**/.husky/_\" \"**/.next*\" \"**/.nuxt*\" \"**/.source*\" \"**/.svelte*\" \"**/.tsbuildinfo\" \"**/.turbo*\" \"**/.vercel\" \"**/.vite*\" \"**/.vscode-*\" \"**/build\" \"**/coverage\" \"**/dist\" \"**/dist-*\" \"**/next-env.d.ts\" \"**/out\" \"**/typegen.d.ts\"",
    "contributors:check": "all-contributors check",
    "contributors:gen": "all-contributors generate",
    "corepack:enable-pnpm": "corepack enable pnpm",
    "corepack:install-pnpm": "corepack install --global pnpm@latest",
    "corepack:use-pnpm": "corepack use pnpm@latest",
    "cz": "czg",
    "dev": "turbo run dev",
    "dev:apps": "turbo run dev --filter=\"./apps/*\"",
    "dev:host": "turbo run dev:host",
    "dev:host:apps": "turbo run dev:host --filter=\"./apps/*\"",
    "dev:host:pkgs": "turbo run dev:host --filter=\"./packages/*\"",
    "dev:pkgs": "turbo run dev --filter=\"./packages/*\"",
    "dlx:check:attw": "pnpx --yes @arethetypeswrong/cli --pack . --ignore-rules cjs-resolves-to-esm",
    "dlx:check:npm": "pnpx @arethetypeswrong/cli --pack .",
    "dlx:check:pkg": "pnpx sort-package-json@latest --check \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "dlx:check:spell": "pnpx cspell-cli@latest lint .",
    "dlx:check:spell:words": "pnpx cspell-cli@latest lint . --words-only --unique --no-progress",
    "dlx:clean:all": "pnpm run dlx:clean:temp ; pnpm run dlx:clean:deps",
    "dlx:clean:deps": "pnpx del-cli@latest \"**/node_modules\" \"**/*-lock.{json,yaml}\" \"**/*.lock\"",
    "dlx:clean:temp": "pnpx del-cli@latest \"**/.*-inspect\" \"**/.*-inspector\" \"**/.astro*\" \"**/.content-collections\" \"**/.contentlayer\" \"**/.eslint-*\" \"**/.husky/_\" \"**/.next*\" \"**/.nuxt*\" \"**/.source*\" \"**/.svelte*\" \"**/.tsbuildinfo\" \"**/.turbo*\" \"**/.vercel\" \"**/.vite*\" \"**/.vscode-*\" \"**/build\" \"**/coverage\" \"**/dist\" \"**/dist-*\" \"**/next-env.d.ts\" \"**/out\" \"**/typegen.d.ts\"",
    "dlx:fix:pkg": "pnpx sort-package-json@latest \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "dlx:init:oxlint": "pnpx oxlint@latest --import-plugin --jest-plugin --jsdoc-plugin --jsx-a11y-plugin --nextjs-plugin --node-plugin --promise-plugin --react-perf-plugin --react-plugin --regex-plugin --vitest-plugin --vue-plugin --tsconfig=./tsconfig.json --init",
    "dlx:upd": "pnpm run dlx:upd:taze ; pnpm run dlx:upd:ncu",
    "dlx:upd:ncu": "pnpx npm-check-updates@latest",
    "dlx:upd:taze": "pnpx taze@latest",
    "fix": "pnpm run \"/^fix:(biome|eslint|md|oxlint|prettier|stylelint)\\$/\"",
    "fix:all": "pnpm run \"/^fix:(biome|eslint|md|oxlint|pkgs|prettier|stylelint)\\$/\"",
    "fix:biome": "biome check --write --max-diagnostics 100",
    "fix:biome:format": "biome format --write --max-diagnostics 100",
    "fix:biome:lint": "biome lint --write --max-diagnostics 100",
    "fix:biome:unsafe": "biome check --write --unsafe --max-diagnostics 100",
    "fix:eslint": "eslint --fix --max-warnings 0 --no-warn-ignored --report-unused-disable-directives-severity warn",
    "fix:knip": "knip --fix",
    "fix:md": "markdownlint --fix \"**/*.md\" \".github/**/*.md\"",
    "fix:oxlint": "oxlint --fix --report-unused-disable-directives-severity=off",
    "fix:pkg": "sort-package-json \"./package.json\" \"./apps/*/package.json\" \"./packages/*/package.json\"",
    "fix:pkgs": "pnpm dedupe",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "fix:stylelint": "stylelint --fix --allow-empty-input --max-warnings 0 --quiet-deprecation-warnings --report-descriptionless-disables --report-invalid-scope-disables --report-needless-disables --report-unscoped-disables \"**/*.{astro,css,html,js,jsx,md,pcss,postcss,scss,styl,stylus,svelte,ts,tsx,vue}\"",
    "fix:unsafe": "pnpm run \"/^fix:(biome:unsafe|eslint|md|oxlint|prettier|stylelint)\\$/\"",
    "init:oxlint": "oxlint --import-plugin --jest-plugin --jsdoc-plugin --jsx-a11y-plugin --nextjs-plugin --node-plugin --promise-plugin --react-perf-plugin --react-plugin --regex-plugin --vitest-plugin --vue-plugin --tsconfig=./tsconfig.json --init",
    "inspect:eslint": "eslint-config-inspector",
    "inspect:modules": "node-modules-inspector",
    "postinstall": "pnpm run build:pkgs",
    "preinstall:disabled": "npx --yes only-allow@latest pnpm",
    "prepare:disable": "husky",
    "test:vi": "vitest",
    "test:vi:cov": "vitest --coverage",
    "test:vi:cov:ui": "vitest --coverage --ui",
    "test:vi:tsc": "vitest --typecheck",
    "test:vi:tsc:ui": "vitest --typecheck --ui",
    "test:vi:ui": "vitest --ui",
    "turbo:telemetry:disable": "turbo telemetry disable",
    "turbo:telemetry:enable": "turbo telemetry enable",
    "turbo:telemetry:status": "turbo telemetry status",
    "upd": "pnpm run upd:taze ; pnpm run upd:ncu",
    "upd:ncu": "npm-check-updates",
    "upd:taze": "taze",
    "upg": "pnpm install --recursive && pnpm update --recursive",
    "upg:latest": "pnpm install --recursive && pnpm update --latest --recursive"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@arethetypeswrong/core": "0.18.2",
    "@astrochemx/commitlint-config": "workspace:*",
    "@astrochemx/cspell-config": "workspace:*",
    "@astrochemx/eslint-config": "workspace:*",
    "@astrochemx/prettier-config": "workspace:*",
    "@astrochemx/stylelint-config": "workspace:*",
    "@astrochemx/typescript-config": "workspace:*",
    "@biomejs/biome": "2.2.2",
    "@carlosjeurissen/stylelint-csstree-validator": "3.2.1",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.6",
    "@commitlint/cli": "19.8.1",
    "@commitlint/format": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@eslint/config-inspector": "1.2.0",
    "@prettier/plugin-xml": "3.4.2",
    "@stylistic/stylelint-plugin": "4.0.0",
    "@turbo/gen": "2.5.6",
    "@types/node": "24.3.1",
    "@types/vscode": "1.103.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "all-contributors-cli": "6.26.1",
    "bumpp": "10.2.3",
    "concurrently": "9.2.1",
    "cspell": "9.2.1",
    "cspell-cli": "9.2.0",
    "czg": "1.12.0",
    "del-cli": "6.0.0",
    "eslint": "9.34.0",
    "husky": "9.1.7",
    "jiti": "2.5.1",
    "knip": "5.63.1",
    "lint-staged": "16.1.6",
    "markdownlint": "0.38.0",
    "markdownlint-cli": "0.45.0",
    "markdownlint-cli2": "0.18.1",
    "node-modules-inspector": "1.1.1",
    "npm-check-updates": "18.0.3",
    "npm-run-all2": "8.0.4",
    "oxlint": "1.14.0",
    "patch-package": "8.0.0",
    "pnpm": "10.15.1",
    "postcss-html": "1.8.0",
    "postcss-less": "6.0.0",
    "postcss-markdown": "1.3.0",
