    "stylelint-declaration-block-no-ignored-properties": "catalog:",
    "stylelint-high-performance-animation": "catalog:",
    "stylelint-less": "catalog:",
    "stylelint-no-unsupported-browser-features": "catalog:",
    "stylelint-order": "catalog:",
